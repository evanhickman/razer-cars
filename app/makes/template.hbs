<div class="section">
  <div class="container">
    <div class="card">
      <div class="card-block card-block__bg">
        <h2 class="card-block__text">Makes</h2>
      </div>
      <div class="card-block">
        <table class="table">
          <tr>
            <th>Manufacturer</th>
            <th>Country</th>
            <th>Available Models</th>
            <th></th>
          </tr>
          {{#each model as |make|}}
          <tr>
            <td>{{make.name}}</td>
            <td>{{make.country}}</td>
            <td>0</td>
            <td><a href="#">See More</a></td>
          </tr>
          {{/each}}
        </table>
      </div>
    </div>
  </div>
</div>

{{outlet}}
